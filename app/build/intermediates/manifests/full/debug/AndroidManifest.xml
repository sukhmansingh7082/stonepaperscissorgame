<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="my.firstapp.sukh"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/images"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="my.firstapp.sukh.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="my.firstapp.sukh.player1"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="my.firstapp.sukh.PLAYER1" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="my.firstapp.sukh.draww"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="my.firstapp.sukh.DRAWW" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="my.firstapp.sukh.player2"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="my.firstapp.sukh.PLAYER2" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="my.firstapp.sukh.rules"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="my.firstapp.sukh.RULES" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="my.firstapp.sukh.developer"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="my.firstapp.sukh.DEVELOPER" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>